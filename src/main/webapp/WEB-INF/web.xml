<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1" metadata-complete="true">
	<welcome-file-list>
		<welcome-file>front/login.html</welcome-file>
	</welcome-file-list>
	
	<!-- jsp-servlet -->
	<servlet>
		<servlet-name>userServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>categoryServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.CategoryServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addressServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.AddressServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>storeServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.StoreServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>productServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.ProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>collectServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.CollectServlet</servlet-class>
	</servlet>
	
	<!-- json-servlet -->
	<servlet>
		<servlet-name>cityServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.CityJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>categoryJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.CategoryJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userTypeJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.UserTypeJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addressJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.AddressJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>collectJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.CollectJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>orderJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.OrderJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>productJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.ProductJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>storeJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.StoreJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userJsonServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.json.UserJsonServlet</servlet-class>
	</servlet>
	
	<!-- upload -->
	<servlet>
		<servlet-name>fileServlet</servlet-name>
		<servlet-class>cn.edu.zhku.xinke.jisuanji.whf.servlet.FileServlet</servlet-class>
	</servlet>
	
	<!-- jsp-servlet mapping -->
	<servlet-mapping>
		<servlet-name>userServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>categoryServlet</servlet-name>
		<url-pattern>/category</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>addressServlet</servlet-name>
		<url-pattern>/address</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>storeServlet</servlet-name>
		<url-pattern>/store</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>productServlet</servlet-name>
		<url-pattern>/product</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>collectServlet</servlet-name>
		<url-pattern>/collect</url-pattern>
	</servlet-mapping>
	
	<!-- json-servlet mapping -->
	<servlet-mapping>
		<servlet-name>cityServlet</servlet-name>
		<url-pattern>/city</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>userTypeJsonServlet</servlet-name>
		<url-pattern>/userTypeJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>categoryJsonServlet</servlet-name>
		<url-pattern>/categoryJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>addressJsonServlet</servlet-name>
		<url-pattern>/addressJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>collectJsonServlet</servlet-name>
		<url-pattern>/collectJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>orderJsonServlet</servlet-name>
		<url-pattern>/orderJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>productJsonServlet</servlet-name>
		<url-pattern>/productJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>storeJsonServlet</servlet-name>
		<url-pattern>/storeJson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>userJsonServlet</servlet-name>
		<url-pattern>/userJson</url-pattern>
	</servlet-mapping>
	
	
	<!-- upload mapping -->
	<servlet-mapping>
		<servlet-name>fileServlet</servlet-name>
		<url-pattern>/file</url-pattern>
	</servlet-mapping>
	
	
	<display-name>java web</display-name>
</web-app>
